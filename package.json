{
  "name": "omgopass-page",
  "version": "1.0.0",
  "description": "Public page for omgopass library",
  "main": "index.js",
  "scripts": {
    "clean-dist": "del-cli 'dist/*' '!dist/demo.gif'",
    "build": "npm run clean-dist && parcel build index.html --public-url /omgopass/",
    "dev": "parcel index.html --open",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "repository": "omgovich/omgopass",
  "author": "Vlad Shilov <omgovich@ya.ru>",
  "license": "ISC",
  "dependencies": {
    "omgopass": "^3.0.0"
  },
  "devDependencies": {
    "del-cli": "^3.0.0",
    "gh-pages": "^2.1.1",
    "parcel-bundler": "^1.12.3"
  }
}
